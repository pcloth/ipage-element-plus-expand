{
    "name": "vuecomp-starter",
    "version": "0.0.1",
    "author": "windlil<windlil@outlook.com>",
    "description": "开源的组件库开发起始模板，帮助以最轻松的方式搭建功能完善的Vue3组件库，让使用者只需关注到组件的开发，无需关注繁琐的搭建配置。",
    "keywords": [
        "components",
        "Vue3",
        "ui"
    ],
    "type": "module",
    "main": "dist/lib/index.js",
    "module": "dist/es/index.js",
    "types": "dist/lib/index.d.ts",
    "exports": {},
    "scripts": {
        "build": "gulp -f packages/script/build/index.ts",
        "auto:npm": "pnpm build && cd dist & npm publish && cd ..",
        "component:create": "esno ./utils/create/index.ts",
        "comp:create": "esno ./utils/createComponent/index.ts",
        "play": "pnpm run -C playground/vue-vite-playground dev",
        "comp:new": "esno ./utils/createTestVue/index.ts",
        "docs:dev": "vitepress dev documents --port 8000",
        "docs:new": "esno ./utils/createVitepressComp/index.ts",
        "docs:build": "vitepress build documents && node ./utils/copyDocsDist.js"
    },
    "files": [
        "dist"
    ],
    "license": "MIT",
    "devDependencies": {
        "@element-plus/build": "workspace:^0.0.1",
        "@element-plus/build-utils": "workspace:^0.0.1",
        "@types/gulp": "^4.0.16",
        "@types/gulp-autoprefixer": "^0.0.36",
        "@types/gulp-less": "^0.0.35",
        "@types/gulp-sass": "^5.0.3",
        "@types/node": "^20.8.7",
        "@vitejs/plugin-vue": "4.0.0",
        "@vitejs/plugin-vue-jsx": "3.0.0",
        "@vue/compiler-sfc": "^3.5.13",
        "@vue/component-compiler-utils": "^3.3.0",
        "chalk": "^5.3.0",
        "dotenv": "^16.3.1",
        "element-plus": "^2.9.1",
        "esno": "^0.17.0",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^8.0.0",
        "gulp-less": "^5.0.0",
        "gulp-sass": "^5.1.0",
        "less": "^4.2.0",
        "markdown-it-anchor": "^9.2.0",
        "markdown-it-chain": "^1.3.0",
        "markdown-it-container": "^4.0.0",
        "markdown-it-toc-done-right": "^4.2.0",
        "sass": "^1.69.4",
        "sucrase": "^3.34.0",
        "transliteration": "^2.3.5",
        "typescript": "^5.2.2",
        "vite": "^4.5.0",
        "vitepress": "^1.5.0",
        "vue": "^3.5.13"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.3.1",
        "@iconify-json/ri": "^1.2.5",
        "@vueuse/core": "^12.3.0",
        "ipage-element-plus-expand": "^0.0.4"
    }
}